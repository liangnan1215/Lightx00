<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIconView?>
<?import de.jensd.fx.glyphs.materialicons.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<StackPane fx:id="dialogContainer" prefHeight="604" prefWidth="804" styleClass="exteriorLine"
           stylesheets="@../helpui/main.css" xmlns="http://javafx.com/javafx/8.0.121"
           xmlns:fx="http://javafx.com/fxml/1"
           fx:controller="trapx00.lightx00.client.presentation.mainui.SaleStaffUiController">
    <AnchorPane style="-fx-background-image: url(../../img/1.jpg); -fx-background-repeat: no-repeat;">
        <HBox AnchorPane.bottomAnchor="0" AnchorPane.leftAnchor="0" AnchorPane.rightAnchor="0" AnchorPane.topAnchor="0">
            <GridPane fx:id="drawer" style="-fx-background-color: white" AnchorPane.bottomAnchor="0"
                      AnchorPane.leftAnchor="0" AnchorPane.topAnchor="0">
                <columnConstraints>
                    <ColumnConstraints prefWidth="200"/>
                </columnConstraints>
                <rowConstraints>
                    <RowConstraints prefHeight="250.0" vgrow="SOMETIMES"/>
                    <RowConstraints prefHeight="50.0" vgrow="SOMETIMES"/>
                </rowConstraints>
                <VBox GridPane.rowIndex="0">
                    <Label fx:id="promptLabel" contentDisplay="CENTER" style="-fx-text-fill: #FFFFFF" text="Hello!">
                        <graphic>
                            <ImageView fitHeight="100.0" fitWidth="200.0">
                                <image>
                                    <Image url="@../../img/2.jpg"/>
                                </image>
                            </ImageView>
                        </graphic>
                    </Label>


                    <JFXButton prefHeight="60" onAction="#onClientFunctionClicked" prefWidth="200" text="管理客户"
                               textFill="#000000">
                        <graphic>
                            <MaterialIconView glyphName="FILTER_TILT_SHIFT" glyphSize="20"/>
                        </graphic>
                    </JFXButton>
                    <JFXButton prefHeight="60" onAction="#onPurchaseBillFunctionClicked" prefWidth="200" text="填写进货单"
                               textFill="#000000">、
                        <graphic>
                            <MaterialIconView glyphName="CONTACTS" glyphSize="20"/>
                        </graphic>
                    </JFXButton>
                    <JFXButton prefHeight="60" onAction="#onPurchaseRefundBillFunctionClicked" prefWidth="200"
                               text="填写进货退货单" textFill="#000000">
                        <graphic>
                            <MaterialIconView glyphName="CHROME_READER_MODE" glyphSize="20"/>
                        </graphic>
                    </JFXButton>
                    <JFXButton prefHeight="60" onAction="#onSaleBillFunctionClicked" prefWidth="200" text="填写销售单"
                               textFill="#000000">
                        <graphic>
                            <MaterialIconView glyphName="GAVEL" glyphSize="20"/>
                        </graphic>
                    </JFXButton>
                    <JFXButton prefHeight="60" onAction="#onSaleRefundBillFunctionClicked" prefWidth="200"
                               text="填写销售退货单" textFill="#000000">
                        <graphic>
                            <MaterialIconView glyphName="LOCAL_GROCERY_STORE" glyphSize="20"/>
                        </graphic>
                    </JFXButton>
                    <Separator prefHeight="5" prefWidth="200"/>
                </VBox>
                <VBox alignment="BOTTOM_CENTER" GridPane.rowIndex="1">
                    <JFXButton onAction="#onNotificationFunctionButtonClicked" prefHeight="60" prefWidth="200" text="主界面" textFill="#000000">
                        <graphic>
                            <FontAwesomeIconView glyphName="HOME" glyphSize="20" />
                        </graphic>
                    </JFXButton>
                    <JFXButton prefHeight="60" prefWidth="200" text="通知" textFill="#000000">
                        <graphic>
                            <MaterialIconView glyphName="MESSAGE" glyphSize="20"/>
                        </graphic>
                    </JFXButton>
                    <JFXButton onAction="#onDraftFunctionButtonClicked" prefHeight="60" prefWidth="200" text="草稿箱"
                               textFill="#000000">
                        <graphic>
                            <MaterialIconView glyphName="CLASS" glyphSize="20"/>
                        </graphic>
                    </JFXButton>
                    <JFXButton prefHeight="60" prefWidth="200" text="关于系统" textFill="#000000">
                        <graphic>
                            <MaterialIconView glyphName="SENTIMENT_VERY_SATISFIED" glyphSize="20"/>
                        </graphic>
                    </JFXButton>
                </VBox>
            </GridPane>
            <BorderPane AnchorPane.bottomAnchor="0" AnchorPane.rightAnchor="0" AnchorPane.topAnchor="0">
                <top>
                    <GridPane fx:id="titleBar" style="-fx-background-color: #232e5f;">
                        <columnConstraints>
                            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="50.0"/>
                            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="50.0"/>
                        </columnConstraints>
                        <rowConstraints>
                            <RowConstraints minHeight="10.0" prefHeight="50.0" vgrow="SOMETIMES"/>
                        </rowConstraints>

                        <HBox alignment="CENTER_LEFT" GridPane.columnIndex="0">
                            <padding>
                                <Insets left="20"/>
                            </padding>
                            <Text fx:id="titleText" fill="white" text="功能">
                                <font>
                                    <Font size="24.0"/>
                                </font>
                            </Text>
                        </HBox>
                        <HBox alignment="CENTER_RIGHT" prefHeight="50" GridPane.columnIndex="1">
                            <JFXButton fx:id="minimizeButton" ripplerFill="aliceblue" text="">
                                <graphic>
                                    <MaterialIconView glyphName="REMOVE" glyphSize="26"
                                                      glyphStyle="-fx-fill: #FFFFFF;"/>
                                </graphic>
                            </JFXButton>
                            <JFXButton fx:id="maximizeButton" ripplerFill="aliceblue" text="">
                                <graphic>
                                    <MaterialIconView fx:id="maximizeButtonGlyph" glyphName="KEYBOARD_ARROW_UP"
                                                      glyphSize="26" glyphStyle="-fx-fill: #FFFFFF;"/>
                                </graphic>
                            </JFXButton>
                            <JFXButton fx:id="closeButton" ripplerFill="red" text="">
                                <graphic>
                                    <MaterialIconView glyphName="CLOSE" glyphSize="26" glyphStyle="-fx-fill: #FFFFFF;"/>
                                </graphic>
                            </JFXButton>
                        </HBox>
                    </GridPane>

                    <!--<HBox fx:id="titleBar" prefHeight="50" style="-fx-background-image: url(../../img/1.jpg);">-->
                    <!--</HBox>-->
                </top>
                <center>
                    <StackPane fx:id="contentPane" alignment="CENTER" styleClass="floatingPane">
                        <BorderPane.margin>
                            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                        </BorderPane.margin>
                    </StackPane>
                </center>
                <bottom>
                    <GridPane fx:id="bottomBar" styleClass="floatingPane" BorderPane.alignment="CENTER">
                        <columnConstraints>
                            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="1000"/>
                            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="1000"/>
                            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="2000"/>
                        </columnConstraints>
                        <rowConstraints>
                            <RowConstraints minHeight="10.0" prefHeight="20.0" vgrow="SOMETIMES"/>
                        </rowConstraints>


                        <HBox alignment="CENTER" GridPane.columnIndex="0">
                            <JFXButton onAction="#onLogButtonClicked" prefWidth="1000.0" text="日志"/>
                        </HBox>
                        <HBox alignment="CENTER" GridPane.columnIndex="1">
                            <JFXButton onAction="#onLogFetchButtonClicked" prefWidth="1000.0" text="取回远程日志"/>
                        </HBox>
                        <HBox alignment="CENTER_RIGHT" spacing="5" GridPane.columnIndex="2">
                            <MaterialIconView glyphName="SIGNAL_CELLULAR_4_BAR" glyphSize="16"
                                              glyphStyle="-fx-background-color: #DDDDDD"/>
                            <Text fx:id="timeText" text="2017/10/25 21:50:00"/>
                        </HBox>
                        <padding>
                            <Insets left="5.0" right="5.0"/>
                        </padding>

                    </GridPane>
                </bottom>
            </BorderPane>
        </HBox>
    </AnchorPane>
</StackPane>
