<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import de.jensd.fx.glyphs.materialicons.MaterialIconView?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<StackPane fx:id="dialogContainer" prefHeight="250.0" prefWidth="360.0" style="-fx-background-color: transparent;" styleClass="exteriorLine" stylesheets="@../helpui/main.css" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="trapx00.lightx00.client.presentation.loginui.LoginUiController">
        <BorderPane fx:id="rootPane" prefHeight="230.0" prefWidth="380.0" style="-fx-background-color: #FFFFFF">
            <BorderPane.margin>
                <Insets bottom="10.0" right="20.0" top="10.0" />
            </BorderPane.margin>
            <top>
                <VBox alignment="CENTER" prefHeight="60.0" prefWidth="300.0">
                    <Label text="Light x00" textFill="#5e5e5e">
                        <font>
                            <Font name="System Bold" size="32.0" />
                        </font>
                        <VBox.margin>
                            <Insets />
                        </VBox.margin>
                    </Label>
                    <Label text="进销存管理系统" textFill="#5e5e5e">
                        <VBox.margin>
                            <Insets left="120.0" />
                        </VBox.margin>
                        <font>
                            <Font size="16.0" />
                        </font>
                    </Label>
            <BorderPane.margin>
               <Insets />
            </BorderPane.margin>
            <padding>
               <Insets bottom="20.0" />
            </padding>
                </VBox>
            </top>
            <center>
                <VBox alignment="CENTER" prefHeight="80.0" spacing="20.0">
                    <HBox alignment="CENTER" spacing="10.0">
                        <MaterialIconView fill="#b4b4b4" glyphName="PERSON" glyphSize="32" />
                        <JFXTextField fx:id="usernameField" labelFloat="true" prefHeight="32" promptText="用户名" />
                    </HBox>
                    <HBox alignment="CENTER" spacing="10.0">
                        <MaterialIconView fill="#b4b4b4" glyphName="SECURITY" glyphSize="24" />
                        <JFXTextField fx:id="passwordField" labelFloat="true" prefHeight="32" promptText="密码" />
                    </HBox>

                </VBox>
            </center>
            <bottom>
                <HBox alignment="CENTER" prefHeight="40.0" spacing="40.0">
                    <JFXButton fx:id="loginButton" buttonType="RAISED" onAction="#onLoginButtonClicked" prefHeight="32.0" prefWidth="80.0" style="-fx-background-color: #3F51B5; -fx-text-fill: #FFFFFF;" text="登 录">
               <HBox.margin>
                  <Insets />
               </HBox.margin></JFXButton>
                    <JFXButton fx:id="cancelButton" buttonType="RAISED" prefHeight="32.0" prefWidth="80.0" style="-fx-background-color: #E8EAF6; -fx-text-fill: #000000;" text="取 消" />
            <BorderPane.margin>
               <Insets top="20.0" />
            </BorderPane.margin>
            <padding>
               <Insets top="10.0" />
            </padding>
                </HBox>
            </bottom>
      <StackPane.margin>
         <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
      </StackPane.margin>
      <padding>
         <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
      </padding>
        </BorderPane>
</StackPane>
